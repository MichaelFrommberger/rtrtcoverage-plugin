				}@/BRANCH@

				@COMMENT@/* Set validity of the message to not available */@/COMMENT@
				tr_saco_global_du_identifiers[loc_ul_du_index].r_load_identifier.b_load_pn_validity = K_SACO_DATA_NOT_AVAIL;
			}@/BRANCH@@/DECISION@

			@COMMENT@/** Acquisition of SN PN identifier message **/@/COMMENT@
            @COMMENT@/*********************************************/@/COMMENT@

			@COMMENT@/* Get du port id in tpl_msg_port_id table according to loc_ul_du_index index */@/COMMENT@
			loc_l_du_port_id = tl_saco_du_id_read_port_id[loc_ul_du_index];

			@COMMENT@/* Get the DU identifiers messages and amendment value from SPORT */@/COMMENT@
			loc_ul_du_read_status = @BRANCH MARK=TA ID=2 SUM=10@@JUMP NAME="read_du_identifier"@read_du_identifier@/JUMP@(	loc_l_du_port_id,
														&loc_r_du_id_message)@/BRANCH@;
			@COMMENT@/* If the read of DU identifier is ok */@/COMMENT@
			@DECISION SUM=20+21 ID=12|25@if (@POPUP@loc_ul_du_read_status== @ALT@K_SACO_DATA_AVAIL@-ALT@0@-POPUP@basic boolean conditions :
  @BRANCH MARK=TE ID=10 SUM=30@True@/BRANCH@
  @BRANCH MARK=TE ID=11 SUM=30@False@/BRANCH@@/POPUP@@/ALT@)
