			@BRANCH MARK=TB ID=35 SUM=20@{
				@COMMENT@/* If msg_validity is VALID */@/COMMENT@
				@DECISION SUM=20+21 ID=33|34@if (@POPUP@loc_e_msg_validity == VALID@-POPUP@basic boolean conditions :
  @BRANCH MARK=TE ID=40 SUM=30@True@/BRANCH@
  @BRANCH MARK=TE ID=41 SUM=30@False@/BRANCH@@/POPUP@)
				@BRANCH MARK=TB ID=33 SUM=20@{
					@COMMENT@/* Set the operation result to data available */@/COMMENT@
					loc_ul_read_status = K_SACO_DATA_AVAIL;
				}@/BRANCH@
				@BRANCH MARK=TB ID=34 SUM=20@else
				{
					@COMMENT@/* Nothing to do */@/COMMENT@
					;
				}@/BRANCH@@/DECISION@
			}@/BRANCH@
			@BRANCH MARK=TB ID=36 SUM=20@else
			{
				@COMMENT@/* Error : Msg size is not correct */@/COMMENT@
				@BRANCH MARK=TA ID=8 SUM=10@@JUMP NAME="lbfl_mng"@lbfl_mng@/JUMP@ ( R_SACO_FSC_ERR_CONFIG,
					K_SACO_FSC_ERR_READ_SAMC_MSG + K_LBFL_E_MULT_OCC,
					K_SACO_SACO_MAIN_COMPONENT_ID, _ERR_FILE_, 0x0,
