@COMMENT@/******************************************************************************
 ***** 
 ***** void saco_receive_data
 ***** 
 ***** Description : - Acquisition of LOAD PN and SN_PN idenitifier message.
 *****						- If no data is received reset the value of the corresponding data
 *****				 - Compute and return #INTER_DU_DUy_COMPATIBILITY_RESULT :
 *****						- Check #DUy_UADFx_COMPATIBILITY_RESULT (data from LOAD PN message)
 *****						- Check HW PN, HW_SN, SW_PN (data from data file)
 *****						- Check if DUy_HW_VERSION are not included in DU_HW_VERSION_INCOMPATIBILITY_TABLE
 *****               
 ***** Input :	T_sadi_du_id	par_e_local_du_id
 ***** 
 ***** 
 ***** Output : none.
 ***** 
 *****************************************************************************/@/COMMENT@
void @NODE TYPE=FUNCTION NAME="saco_receive_data"@@BRANCH MARK=TP ID=1 SUM=0@@LINK NAME="saco_receive_data"@saco_receive_data@/BRANCH@ (T_sadi_du_id	par_e_local_du_id)
@BRANCH MARK=TB ID=32 SUM=20@{
	T_int32										loc_l_i;
	SAMPLING_PORT_ID_TYPE						loc_l_du_port_id;	
	T_uint32									loc_ul_du_read_status;
	T_uint32									loc_ul_du_index;
	T_uint32									loc_ul_uadfx;
	T_uint8										loc_uc_identifier_index;
	T_saco_du_identifier_message_format		    loc_r_du_id_message;
	T_saco_du_load_info_format					loc_r_load_pn_msg;
    T_uint8                                     loc_uc_saco_nb_dcp_identifiers_index;

	@COMMENT@/** - Acquisition of LOAD PN and SN_PN identifier message **/@/COMMENT@

	@POPUP@for@-POPUP@loop branches :
@BRANCH MARK=TB ID=29 SUM=22@  0 loop@/BRANCH@
@BRANCH MARK=TB ID=30 SUM=22@  1 loop@/BRANCH@
@BRANCH MARK=TB ID=31 SUM=22@  2 loops or more@/BRANCH@@/POPUP@ (loc_l_i=0; @POPUP@loc_l_i < @ALT@K_SACO_MAX_DU_ID_PORT@-ALT@8@-POPUP@basic boolean conditions :
  @BRANCH MARK=TE ID=0 SUM=30@True@/BRANCH@
  @BRANCH MARK=TE ID=1 SUM=30@False@/BRANCH@@/POPUP@@/ALT@; loc_l_i++)
	@BRANCH MARK=TB ID=28 SUM=20@{
		@COMMENT@/* If the DU identifier port to read is not the read port of current DU  */@/COMMENT@
		@DECISION SUM=20+21 ID=26|27@if ( @POPUP@TR_SACO_DU_LOAD_PN_RPORT_CREAT[loc_l_i].ul_saco_du_id !=  (T_uint32) par_e_local_du_id@-POPUP@basic boolean conditions :
  @BRANCH MARK=TE ID=2 SUM=30@True@/BRANCH@
  @BRANCH MARK=TE ID=3 SUM=30@False@/BRANCH@@/POPUP@)
		@BRANCH MARK=TB ID=26 SUM=20@{
			
			@COMMENT@/** Acquisition of LOAD PN message **/@/COMMENT@
            @COMMENT@/************************************/@/COMMENT@
			
			@COMMENT@/* Get DU index in DU LOAD PN read port table */@/COMMENT@
			loc_ul_du_index = TR_SACO_DU_LOAD_PN_RPORT_CREAT[loc_l_i].ul_port_index;

			@COMMENT@/* Get du port id in tl_saco_du_load_pn_read_port_id table according to loc_ul_du_index index */@/COMMENT@
			loc_l_du_port_id = tl_saco_du_load_pn_read_port_id[loc_ul_du_index];

			@COMMENT@/* Get the load PN message from SPORT */@/COMMENT@
			loc_ul_du_read_status = @BRANCH MARK=TA ID=0 SUM=10@@JUMP NAME="read_load_pn_message"@read_load_pn_message@/JUMP@(	loc_l_du_port_id,
															&loc_r_load_pn_msg)@/BRANCH@;
			@COMMENT@/* If the message is available */@/COMMENT@
			@DECISION SUM=20+21 ID=4|9@if (@POPUP@loc_ul_du_read_status== @ALT@K_SACO_DATA_AVAIL@-ALT@0@-POPUP@basic boolean conditions :
  @BRANCH MARK=TE ID=4 SUM=30@True@/BRANCH@
  @BRANCH MARK=TE ID=5 SUM=30@False@/BRANCH@@/POPUP@@/ALT@)
			@BRANCH MARK=TB ID=4 SUM=20@{


				@COMMENT@/* Update #DUy_UADFx_VERSION and validity of the message in the global variable */@/COMMENT@
				tr_saco_global_du_identifiers[loc_ul_du_index].r_load_identifier.b_load_pn_validity = K_SACO_DATA_AVAIL;

				@POPUP@for@-POPUP@loop branches :
@BRANCH MARK=TB ID=1 SUM=22@  0 loop@/BRANCH@
@BRANCH MARK=TB ID=2 SUM=22@  1 loop@/BRANCH@
@BRANCH MARK=TB ID=3 SUM=22@  2 loops or more@/BRANCH@@/POPUP@ (loc_ul_uadfx = K_SACO_UADF_FMS_PN_I; @POPUP@loc_ul_uadfx <= K_SACO_UADF_MMMS_MTAD_C_PN_I@-POPUP@basic boolean conditions :
  @BRANCH MARK=TE ID=6 SUM=30@True@/BRANCH@
  @BRANCH MARK=TE ID=7 SUM=30@False@/BRANCH@@/POPUP@; loc_ul_uadfx++)
				@BRANCH MARK=TB ID=0 SUM=20@{
					@BRANCH MARK=TA ID=1 SUM=10@@JUMP NAME="lbstr_cpy"@lbstr_cpy@/JUMP@ ( tr_saco_global_du_identifiers[loc_ul_du_index].r_load_identifier.tr_load_pn_value[loc_ul_uadfx].tuc_part_number,
								loc_r_load_pn_msg.tuc_uadf_pn[loc_ul_uadfx-K_SACO_UADF_FMS_PN_I].tuc_part_number)@/BRANCH@;			
				}@/BRANCH@
			}@/BRANCH@
			@BRANCH MARK=TB ID=9 SUM=20@else 
			{
				@COMMENT@/* Reset DUy_UADFx_VERSION (message not received) */@/COMMENT@
				@POPUP@for@-POPUP@loop branches :
@BRANCH MARK=TB ID=6 SUM=22@  0 loop@/BRANCH@
@BRANCH MARK=TB ID=7 SUM=22@  1 loop@/BRANCH@
@BRANCH MARK=TB ID=8 SUM=22@  2 loops or more@/BRANCH@@/POPUP@ (loc_ul_uadfx = K_SACO_UADF_FMS_PN_I; @POPUP@loc_ul_uadfx <= K_SACO_UADF_MMMS_MTAD_C_PN_I@-POPUP@basic boolean conditions :
  @BRANCH MARK=TE ID=8 SUM=30@True@/BRANCH@
  @BRANCH MARK=TE ID=9 SUM=30@False@/BRANCH@@/POPUP@; loc_ul_uadfx++)
				@BRANCH MARK=TB ID=5 SUM=20@{
