		@BRANCH MARK=TB ID=27 SUM=20@else
		{
			@COMMENT@/* Nothing to do for the local DU */@/COMMENT@
			;
		}@/BRANCH@@/DECISION@
	}@/BRANCH@

    @BRANCH MARK=TP ID=0 SUM=1@return;@SHORTCUT MARK=TB ID=32@@/BRANCH@
