				@COMMENT@/* Get DU identifier index value */@/COMMENT@
				loc_uc_identifier_index = loc_r_du_id_message.uc_identifier_index;

				@COMMENT@/* If the DU identifier index is correct */@/COMMENT@
				@DECISION SUM=20+21 ID=10|11@if (@POPUP@loc_uc_identifier_index < K_SACO_MAX_NB_DU_IDENTIFIERS@-POPUP@basic boolean conditions :
  @BRANCH MARK=TE ID=12 SUM=30@True@/BRANCH@
  @BRANCH MARK=TE ID=13 SUM=30@False@/BRANCH@@/POPUP@)
				@BRANCH MARK=TB ID=10 SUM=20@{
					@COMMENT@/* Update the Amendment from SPORT in the global variable */@/COMMENT@
					@BRANCH MARK=TA ID=3 SUM=10@@JUMP NAME="lbstr_cpy"@lbstr_cpy@/JUMP@ ( tr_saco_global_du_amendment[loc_ul_du_index].tuc_amendment ,
								loc_r_du_id_message.tuc_amendment_value.tuc_amendment)@/BRANCH@;

					@COMMENT@/* Update the DU identifiers from SPORT in the specific DU identifier global variable */@/COMMENT@
					@BRANCH MARK=TA ID=4 SUM=10@@JUMP NAME="lbstr_cpy"@lbstr_cpy@/JUMP@ ( tr_saco_global_du_identifiers[loc_ul_du_index].r_data_file_identifier.tr_sn_pn_value[loc_uc_identifier_index].tuc_du_identifier_sn_pn,
						loc_r_du_id_message.tuc_identifier_value)@/BRANCH@;

					@COMMENT@/* Data is available */@/COMMENT@
					tr_saco_global_du_identifiers[loc_ul_du_index].r_data_file_identifier.uc_sn_pn_validity[loc_uc_identifier_index] = K_SACO_DATA_AVAIL;

                    loc_uc_saco_nb_dcp_identifiers_index = loc_uc_identifier_index%2;

                    @COMMENT@/* Compute DCP SN and PN */@/COMMENT@
                    @BRANCH MARK=TA ID=5 SUM=10@@JUMP NAME="compute_dcp_pn_sn"@compute_dcp_pn_sn@/JUMP@( loc_l_i,
                                       loc_uc_saco_nb_dcp_identifiers_index,
                                       &loc_r_du_id_message)@/BRANCH@;
  				}@/BRANCH@
				@BRANCH MARK=TB ID=11 SUM=20@else
				{
					@COMMENT@/* Error : wrong DU identifier index */@/COMMENT@
					@BRANCH MARK=TA ID=6 SUM=10@@JUMP NAME="lbfl_mng"@lbfl_mng@/JUMP@ (	R_SACO_FSC_ERR_CONFIG,
								 K_SACO_FSC_ERR_IDENTIFIER_ID,
								 K_SACO_SACO_MAIN_COMPONENT_ID_ATT,
								 (T_uint32)loc_uc_identifier_index,
								 0,
								 0,
								 K_NULL_PTR,
								 0)@/BRANCH@;
				}@/BRANCH@@/DECISION@
			}@/BRANCH@
			@BRANCH MARK=TB ID=25 SUM=20@else
			{
				@COMMENT@/* Reset SN_PN when not received */@/COMMENT@
				@POPUP@for@-POPUP@loop branches :
@BRANCH MARK=TB ID=14 SUM=22@  0 loop@/BRANCH@
@BRANCH MARK=TB ID=15 SUM=22@  1 loop@/BRANCH@
@BRANCH MARK=TB ID=16 SUM=22@  2 loops or more@/BRANCH@@/POPUP@ (loc_uc_identifier_index = K_SACO_DU_HW_PN_I; @POPUP@loc_uc_identifier_index< K_SACO_MAX_NB_DU_IDENTIFIERS@-POPUP@basic boolean conditions :
  @BRANCH MARK=TE ID=14 SUM=30@True@/BRANCH@
  @BRANCH MARK=TE ID=15 SUM=30@False@/BRANCH@@/POPUP@; loc_uc_identifier_index ++)
				@BRANCH MARK=TB ID=13 SUM=20@{
					*tr_saco_global_du_identifiers[loc_ul_du_index].r_data_file_identifier.tr_sn_pn_value[loc_uc_identifier_index].tuc_du_identifier_sn_pn = K_SACO_EMPTY_WORD_DATA;

					@COMMENT@/* Set validity of du identifier to not available */@/COMMENT@
					tr_saco_global_du_identifiers[loc_ul_du_index].r_data_file_identifier.uc_sn_pn_validity[loc_uc_identifier_index] = K_SACO_DATA_NOT_AVAIL;
				}@/BRANCH@
                @COMMENT@/* update PCP information priority */@/COMMENT@
                @DECISION SUM=20+21 ID=17|20@if (@POPUP@TR_SACO_DU_LOAD_PN_RPORT_CREAT[loc_l_i].ul_saco_du_id == K_SADI_DU_OL@-POPUP@basic boolean conditions :
  @BRANCH MARK=TE ID=16 SUM=30@True@/BRANCH@
  @BRANCH MARK=TE ID=17 SUM=30@False@/BRANCH@@/POPUP@)
                @BRANCH MARK=TB ID=17 SUM=20@{
                    r_saco_dcp_OL_available = K_BOOL_FALSE;
                }@/BRANCH@
                @BRANCH MARK=TB ID=20 SUM=20@else @DECISION SUM=20+21 ID=18|19@if (@POPUP@TR_SACO_DU_LOAD_PN_RPORT_CREAT[loc_l_i].ul_saco_du_id == K_SADI_DU_OR@-POPUP@basic boolean conditions :
  @BRANCH MARK=TE ID=18 SUM=30@True@/BRANCH@
  @BRANCH MARK=TE ID=19 SUM=30@False@/BRANCH@@/POPUP@)
                @BRANCH MARK=TB ID=18 SUM=20@{
                    r_saco_dcp_OR_available = K_BOOL_FALSE;
                }@/BRANCH@
                @BRANCH MARK=TB ID=19 SUM=20@else
                {
                    @COMMENT@/* NOTHING TO DO */@/COMMENT@
                }@/BRANCH@@/DECISION@@/BRANCH@@/DECISION@
                @COMMENT@/* Reset DCP SN and PN when IL or IR are off and message with invalid PN and SN from OL or OR */@/COMMENT@
                @DECISION SUM=20+21 ID=21|24@if (@POPUP@(@ALT@K_BOOL_FALSE@-ALT@@POPUP@0UL@/ALT@ == r_saco_dcp_OL_available@-POPUP@basic boolean conditions :
  @BRANCH MARK=TE ID=20 SUM=30@True@/BRANCH@
  @BRANCH MARK=TE ID=21 SUM=30@False@/BRANCH@@/POPUP@) && (@POPUP@TR_SACO_DU_LOAD_PN_RPORT_CREAT[loc_l_i].ul_saco_du_id == K_SADI_DU_IL@-POPUP@basic boolean conditions :
  @BRANCH MARK=TE ID=22 SUM=30@True@/BRANCH@
  @BRANCH MARK=TE ID=23 SUM=30@False@/BRANCH@@/POPUP@)@-POPUP@modified and multiple conditions :
  @BRANCH MARK=TE ID=24&26 SUM=31@0UL == r_saco_dcp_OL_available@/BRANCH@ needs
      @BRANCH MARK=TE ID=24 SUM=32@TT@/BRANCH@=T,@BRANCH MARK=TE ID=26 SUM=32@FX@/BRANCH@=F
  @BRANCH MARK=TE ID=24&25 SUM=31@TR_SACO_DU_LOAD_PN_RPORT_CREAT[loc_l_i].ul_saco_du_id ==  ...@/BRANCH@ needs
      @BRANCH MARK=TE ID=24 SUM=32@TT@/BRANCH@=T,@BRANCH MARK=TE ID=25 SUM=32@TF@/BRANCH@=F@/POPUP@)
                @BRANCH MARK=TB ID=21 SUM=20@{
                    r_saco_eqpt_identifiers [K_SACO_SIDE_LEFT][K_SACO_EQPT_DCP].pn.identifier [0] = 0x00;
                    r_saco_eqpt_identifiers [K_SACO_SIDE_LEFT][K_SACO_EQPT_DCP].pn.validity = K_BOOL_FALSE;
                    r_saco_eqpt_identifiers [K_SACO_SIDE_LEFT][K_SACO_EQPT_DCP].sn.identifier [0] = 0x00;
                    r_saco_eqpt_identifiers [K_SACO_SIDE_LEFT][K_SACO_EQPT_DCP].sn.validity = K_BOOL_FALSE;
                }@/BRANCH@
                @BRANCH MARK=TB ID=24 SUM=20@else @DECISION SUM=20+21 ID=22|23@if (@POPUP@(@ALT@K_BOOL_FALSE@-ALT@@POPUP@0UL@/ALT@ == r_saco_dcp_OR_available@-POPUP@basic boolean conditions :
  @BRANCH MARK=TE ID=27 SUM=30@True@/BRANCH@
  @BRANCH MARK=TE ID=28 SUM=30@False@/BRANCH@@/POPUP@) && (@POPUP@TR_SACO_DU_LOAD_PN_RPORT_CREAT[loc_l_i].ul_saco_du_id == K_SADI_DU_IR@-POPUP@basic boolean conditions :
  @BRANCH MARK=TE ID=29 SUM=30@True@/BRANCH@
  @BRANCH MARK=TE ID=30 SUM=30@False@/BRANCH@@/POPUP@)@-POPUP@modified and multiple conditions :
  @BRANCH MARK=TE ID=31&33 SUM=31@0UL == r_saco_dcp_OR_available@/BRANCH@ needs
      @BRANCH MARK=TE ID=31 SUM=32@TT@/BRANCH@=T,@BRANCH MARK=TE ID=33 SUM=32@FX@/BRANCH@=F
  @BRANCH MARK=TE ID=31&32 SUM=31@TR_SACO_DU_LOAD_PN_RPORT_CREAT[loc_l_i].ul_saco_du_id ==  ...@/BRANCH@ needs
      @BRANCH MARK=TE ID=31 SUM=32@TT@/BRANCH@=T,@BRANCH MARK=TE ID=32 SUM=32@TF@/BRANCH@=F@/POPUP@)
                @BRANCH MARK=TB ID=22 SUM=20@{
                    r_saco_eqpt_identifiers [K_SACO_SIDE_RIGHT][K_SACO_EQPT_DCP].pn.identifier [0] = 0x00;
                    r_saco_eqpt_identifiers [K_SACO_SIDE_RIGHT][K_SACO_EQPT_DCP].pn.validity = K_BOOL_FALSE;
                    r_saco_eqpt_identifiers [K_SACO_SIDE_RIGHT][K_SACO_EQPT_DCP].sn.identifier [0] = 0x00;
                    r_saco_eqpt_identifiers [K_SACO_SIDE_RIGHT][K_SACO_EQPT_DCP].sn.validity = K_BOOL_FALSE;
                }@/BRANCH@
                @BRANCH MARK=TB ID=23 SUM=20@else
                {
                    @COMMENT@/* NOTHING TO DO */@/COMMENT@
                }@/BRANCH@@/DECISION@@/BRANCH@@/DECISION@

			}@/BRANCH@@/DECISION@
