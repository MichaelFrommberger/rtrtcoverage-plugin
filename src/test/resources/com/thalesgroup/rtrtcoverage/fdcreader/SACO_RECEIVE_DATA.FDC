			par_l_port_id,
			0,
			0,
			K_NULL_PTR,
			0)@/BRANCH@;
	}@/BRANCH@@/DECISION@
	@COMMENT@/* Endif */@/COMMENT@

	@BRANCH MARK=TP ID=2 SUM=1@return(loc_ul_read_status);@SHORTCUT MARK=TB ID=43@@/BRANCH@
