					(T_uint32)loc_l_msg_size,
					0,
					K_NULL_PTR,
					0)@/BRANCH@;
			}@/BRANCH@@/DECISION@
		}@/BRANCH@
		@BRANCH MARK=TB ID=40 SUM=20@else @DECISION SUM=20+21 ID=38|39@if (@POPUP@loc_e_msg_return_code == NO_ACTION@-POPUP@basic boolean conditions :
  @BRANCH MARK=TE ID=42 SUM=30@True@/BRANCH@
  @BRANCH MARK=TE ID=43 SUM=30@False@/BRANCH@@/POPUP@)
		@BRANCH MARK=TB ID=38 SUM=20@{
			@COMMENT@/* Nothing to do */@/COMMENT@
			;
		}@/BRANCH@
		@BRANCH MARK=TB ID=39 SUM=20@else
		{
			@COMMENT@/* Raise generic error */@/COMMENT@
			@BRANCH MARK=TA ID=9 SUM=10@@JUMP NAME="lbfl_mng"@lbfl_mng@/JUMP@ (R_SACO_FSC_ERR_CONFIG,
				K_LBFL_E_GNERR_SERV_SAMPLING_PORT_READ + K_LBFL_E_MULT_OCC,
				K_SACO_SACO_MAIN_COMPONENT_ID, _ERR_FILE_, 0x0,
				loc_e_msg_return_code, @COMMENT@/* Return code */@/COMMENT@
				loc_e_msg_validity,	   @COMMENT@/* Validity */@/COMMENT@ 
				0,
				K_NULL_PTR,
				0)@/BRANCH@;	

		}@/BRANCH@@/DECISION@@/BRANCH@@/DECISION@
	}@/BRANCH@
	@COMMENT@/* Else */@/COMMENT@
	@BRANCH MARK=TB ID=42 SUM=20@else
	{
		@COMMENT@/* Error : Invalid msg port id */@/COMMENT@
		@BRANCH MARK=TA ID=10 SUM=10@@JUMP NAME="lbfl_mng"@lbfl_mng@/JUMP@ (R_SACO_FSC_ERR_CONFIG,
			K_LBFL_E_GNERR_SERV_UNEXPECTED_VALUE + K_LBFL_E_MULT_OCC,
