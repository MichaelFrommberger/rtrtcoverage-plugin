        @BRANCH MARK=TB ID=4 SUM=20@@BRANCH NO@@BRANCH MARK=TE ID=4 SUM=30@case K_SADI_DU_LL:@/BRANCH@@/BRANCH@
            {
                loc_du1_id = K_SADI_DU_OL;
                loc_du2_id = K_SADI_DU_IL;
            }
            break;@SHORTCUT MARK=TB ID=0@

        @BRANCH MARK=TB ID=5 SUM=20@@BRANCH NO@@BRANCH MARK=TE ID=5 SUM=30@case K_SADI_DU_LR:@/BRANCH@@/BRANCH@
            {
                loc_du1_id = K_SADI_DU_OR;
                loc_du2_id = K_SADI_DU_IR;
            }
            break;@SHORTCUT MARK=TB ID=0@

        @BRANCH NO@@BRANCH MARK=TE ID=6 SUM=30@case K_SADI_DU_CL:@/BRANCH@@/BRANCH@
        @BRANCH NO@@BRANCH MARK=TE ID=7 SUM=30@case K_SADI_DU_CR:@/BRANCH@@/BRANCH@
        @BRANCH NO@@BRANCH MARK=TE ID=8 SUM=30@case K_SADI_DU_3C:@/BRANCH@@/BRANCH@
        @BRANCH MARK=TB ID=6 SUM=20@@BRANCH NO@@BRANCH MARK=TE ID=9 SUM=30@default :@/BRANCH@@/BRANCH@
            {
                loc_du1_id = K_SADI_DU_NONE;
                loc_du2_id = K_SADI_DU_NONE;
